{
  "headers": 
  [
    {
      "source" : "**/*.@(wasm.gz)",
      "headers" : [
        {
        "key" : "Content-Encoding",
        "value" : "gzip"},
        {
          "key" : "Content-Type",
          "value" : "application/wasm"
        }
      ]
    }, 
    {
      "source" : "**/*.@(data.gz)",
      "headers" : [
        {
        "key" : "Content-Encoding",
        "value" : "gzip"},
        {
          "key" : "Content-Type",
          "value" : "application/octet-stream"
        }
      ]
    }, 
    {
      "source" : "**/*.@(js.gz)",
      "headers" : [
        {
        "key" : "Content-Encoding",
        "value" : "gzip"},
        {
          "key" : "Content-Type",
          "value" : "application/javascript"
        }
      ]
    }
  ]
}